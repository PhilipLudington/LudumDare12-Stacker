<UserControl x:Class="LD12.Rectangle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    >
    <Canvas>
        <Rectangle x:Name="rectangle" Fill="Yellow" Stroke="Black" StrokeThickness="2" Width="10" Height="10">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <TranslateTransform x:Name="translateTransform"  X="-5" Y="-5"/>
                    <RotateTransform x:Name="rotateTransform"/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
    </Canvas>
</UserControl>
